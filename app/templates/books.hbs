 <h1>Books</h1>

 <table class="table table-bordered table-striped">
   <thead>
     <tr>
       <th>Title</th>
       <th>Release Date</th>
     </tr>
   </thead>
   <tbody>
     {{#each model as |book|}}
       <tr>
         <td>{{book.title}}</td>
         <td>
           <ul>
               <li>{{book.releaseYear}}</li>
           </ul>
         </td>
       </tr>
     {{/each}}
   </tbody>
 </table>